{
    "name": "millancore/pipeline-iterator",
    "description": "Pipeline Iterator Filter",
    "type": "library",
    "version": "1.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Millan",
            "email": "juanes14569@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "Millancore\\PipelineIterator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Millancore\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse",
        "fix": "php-cs-fixer fix ."
    }
}
